    "babel": "^5.8.21",
    "express": "^4.13.3",
    "react": "^0.13.3",
    "react-router": "^1.0.0-rc1",
    "redux-router": "^1.0.0-beta3",
    "react-redux": "^2.1.2",
    "react-dom": "^0.14.0-rc1",
    "redux": "^3.0.0",
    "redux-logger" : "^2.0.2",
    "redux-thunk": "^0.1.0",
    "redux-undo": "^0.5.0",
    "history": "^1.9.0",
    "classnames" : "^2.1.5",
    "axios" : "^0.7.0",
    "react-transform-hmr": "^1.0.0",
