
{
  "name": "simple-redux-boiler",
  "version": "0.0.8",
  "description": "An example of a universally-rendered Redux application",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development node src/server/index.js --progress --colors --profile",
    "start-prod": "NODE_ENV=production node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caljrimmer/simple-redux-boiler.git"
  },
  "license": "MIT",
